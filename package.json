{
  "name": "jsx-create-element",
  "version": "0.0.7",
  "description": "jsx to document.createElement",
  "main": "index.js",
  "scripts": {
    "babel": "babel ./src --out-dir ./lib",
    "babel-test": "babel ./test/in/test.jsx > ./test/out/result.js",
    "build-test": "node test/build",
    "create-index": "echo 'module.exports = require(\"./lib\").default;' > index.js",
    "pretest": "node build.js && npm run create-index && npm run babel-test && npm run build-test && npm run babel",
    "prerelease": "npm test",
    "release": "standard-version --no-verify",
    "test": "node test/test.js"
  },
  "author": "Gregor Adams <greg@pixelass.com> (http://pixelass.com)",
  "keywords": [
    "jsx",
    "dom"
  ],
  "repository": "https://github.com/pixelass/jsx-create-element",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "colors": "^1.1.2",
    "css-modulesify": "^0.25.1",
    "cz-conventional-changelog": "^1.1.7",
    "errorify": "^0.3.1",
    "git-scripts": "^0.2.1",
    "log": "^1.4.0",
    "standard-version": "^2.4.0",
    "watchify": "^3.7.0"
  },
  "git": {
    "scripts": {
      "pre-commit": "npm test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015",
            "stage-0"
          ]
        }
      ]
    ]
  }
}
